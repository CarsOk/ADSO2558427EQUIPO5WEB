<div class="form-container">
  <div class="form-container2">
    <h2 style="text-align: center;"><%= @titulo %></h2>
    <%= simple_form_for @cliente do |f| %>
      <div class="form-group row">
          <%= f.label :identificacion, "Identificación" %>
          <%= f.text_field :identificacion, class: "form-control" %>
      </div>
      <div class="form-group row">
          <%= f.label :nombre, "Nombre" %>
          <%= f.text_field :nombre, class: "form-control" %>
      </div>
      <div class="form-group row">
          <%= f.label :apellido, "Apellido" %>
          <%= f.text_field :apellido, class: "form-control" %>
      </div>
      <div class="form-group row">
          <%= f.label :telefono, "Teléfono" %>
          <%= f.text_field :email, class: "form-control" %>
      </div>
      <div class="form-group">
          <%= f.submit "Enviar", class: "btn btn-primary" %>
      </div>
    <% end %>
  </div>
</div>